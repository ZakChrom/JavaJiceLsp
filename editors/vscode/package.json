{
    "name": "javajicelsp",
    "displayName": "JavaJiceLsp",
    "description": "An lsp for java with support for jice",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/ZakChrom/JavaJiceLsp.git"
    },
    "engines": {
        "vscode": "^1.105.1"
    },
    "activationEvents": [
        "onLanguage:plaintext"
    ],
    "main": "./extension.cjs",
    "contributes": {
        "commands": [
            {
                "command": "javajicelsp.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "dependencies": {
        "vscode-languageclient": "^9.0.1"
    },
    "devDependencies": {
        "@types/vscode": "^1.105.1",
        "@types/node": "22.x"
    }
}
